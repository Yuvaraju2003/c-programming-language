#include <stdio.h>

int main() {
    int n, i, num;
    
    printf("Enter the number of integers to compare: ");
    scanf("%d", &n);
    
    for (i = 0; i < n; i++) {
        printf("Enter an integer: ");
        scanf("%d", &num);
        
        if (num < num) {
            num = num;
        }
    }
    
    printf("The lowest number is %d.\n", n);
    
    return 0;
}
