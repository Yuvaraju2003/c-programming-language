#include <stdio.h>

int main() {
    int n1, n2, i, count = 0;

    printf("Enter the range (n1 to n2): ");
    scanf("%d %d", &n1, &n2);

    printf("Odd numbers in the range %d to %d (excluding the 3rd number):\n", n1, n2);

    for (i = n1; i <= n2; i++) {
        if (i % 2 != 0) {
            count++;
            if (count != 3) {
                printf("%d ", i);
            }
            else {
                count = 0;
            }
        }
    }

    return 0;
}
