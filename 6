#include <stdio.h>
#include <math.h>

int isArmstrong(int n) {
    int sum = 0, original = n, count = 0;
    while(original != 0) {
        count++;
        original /= 10;
    }
    original = n;
    while(original != 0) {
        int digit = original % 10;
        sum += pow(digit, count);
        original /= 10;
    }
    return sum == n;
}

