#include <stdio.h>

int isPrime(int n) {
    if(n <= 1) return 0;
    if(n <= 3) return 1;
    if(n % 2 == 0 || n % 3 == 0) return 0;
    for(int i = 5; i * i <= n; i += 6) {
        if(n % i == 0 || n % (i + 2) == 0) {
            return 0;
        }
    }
    return 1;
}

int main() {
    int start, end;
    printf("Enter the starting and ending numbers: ");
    scanf("%d %d", &start, &end);
    int count = 0;
    for(int i = start; i <= end; i++) {
        if(isPrime(i)) {
            count++;
        }
    }
    printf("The number of prime numbers between %d and %d is: %d", start, end, count);
    return 0;
}
