#include <stdio.h>

int main() {
    int rows = 3, cols = 4;
    int start = 1, diff = 2;
    
    for (int i = 0; i < rows; i++) {
        int num = start + i * diff;
        for (int j = 0; j < cols; j++) {
            printf("%-3d", num);
            num += diff;
        }
        printf("\n");
    }
    
    return 0;
}
